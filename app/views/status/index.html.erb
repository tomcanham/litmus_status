<div class="site-status status <%= status_to_class(@latest_status) %>">
  Site is: <%= status_to_text(@latest_status) %>
  <span class="timestamp">as of <%= @latest_status.updated_at %></span>
</div>

<h1>Latest (5) status updates</h1>
<div>
  <% @latest.each do |status| %>
    <%= render status %>
  <% end %>
</div>